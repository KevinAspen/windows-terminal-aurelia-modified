{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "final_space": true,
    "blocks":
    [
        {
            "type": "prompt",
            "alignment": "left",
            "segments":
            [
                {
                    "type": "text",
                    "style": "plain",
                    "template": "<#FF23FF>‚ï≠[</> "
                },
                {
                    "type": "os",
                    "style": "powerline",
                    "powerline_symbol": "ÓÇº",
                    "foreground": "#FFFFFF",
                    "background": "#7F00FF",
                    "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
                },
                {
                    "type": "session",
                    "style": "powerline",
                    "powerline_symbol": "ÓÉÜ",
                    "foreground": "#ffffff",
                    "background": "#9F33FF",
                    "template": " {{ if .SSHSession }}Ô†ó {{ end }}{{ .UserName }}@{{ .HostName }} ",
                    "properties":
                    {
                        "display_host": true,
                        "display_user": true
                    }
                },
                {
                    "type": "dotnet",
                    "style": "powerline",
                    "powerline_symbol": "ÓÉÄ",
                    "foreground": "#FFFFFF",
                    "background": "#C1137B",
                    "template": " Óùø {{ if .Unsupported }}ÔÅ±{{ else }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "ÓÉÄ",
                    "foreground": "#ffffff",
                    "background": "#E92887",
                    "template": " Óúò {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
                },
                {
                    "type": "exit",
                    "style": "plain",
                    "powerline_symbol": "ÓÇ∞",
                    "foreground": "#ffffff",
                    "template": "<#FF23FF>[x</>{{ if gt .Code 0 }}ÔÄç {{ .Meaning }}{{ else }}ÔêÆ{{ end }}<#FF23FF>] </>"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments":
            [
                {
                    "type": "root",
                    "style": "plain",
                    "foreground": "#B5B50D",
                    "template": "<#FF23FF>[</>ÔÉß<#FF23FF>] </>"
                },
                {
                    "type": "spotify",
                    "style": "diamond",
                    "foreground": "#ffffff",
                    "background": "#1BD760",
                    "leading_diamond": "ÓÇ∂",
                    "trailing_diamond": "ÓÇ¥",
                    "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} "
                },
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#007ACC",
                    "template": "<#FF23FF>[üïí</>{{ .CurrentDate | date .Format }}<#FF23FF>] </>"
                }
            ]
        },
        {
            "type": "newline"
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments":
            [
                {
                    "type": "text",
                    "style": "plain",
                    "template": "<#FF23FF>‚ï∞[</> "
                },
                {
                    "type": "path",
                    "style": "powerline",
                    "powerline_symbol": "ÓÇ∏",
                    "foreground": "#ffffff",
                    "background": "#7F00FF",
                    "template": " üìÅ{{ .Path }} ",
                    "properties":
                    {
                        "style": "folder"
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "ÓÇ∞",
                    "foreground": "#ffffff",
                    "background": "#9F33FF",
                    "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }} ",
                    "properties":
                    {
                        "fetch_stash_count": true,
                        "fetch_upstream_icon": true
                    }
                }
            ]
        }
    ]
}
